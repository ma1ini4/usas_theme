<div class="mz-item-tracking">
  <div class="mz-tracking-title">{{labels.orderStatusTrackings}}</div>
  <div>{{model.itemStatus}}</div>
  <div>{{model.itemTrackingCarrierName}}</div>
  <div>{{model.itemTrackingCode}}</div>
  <div>{{model.itemTrackingDate|date("F j, Y") }}</div>
  <div><a href="{{model.itemTrackingCarrierURL}}">{{model.itemTrackingNumber}}</a></div>
</div>
