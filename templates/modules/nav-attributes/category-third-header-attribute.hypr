{% if model %}
	{% with model|first as entityModel %}
		{% if entityModel.topNavHeader3 != '' or entityModel.topNavHeader3 == '~' %}
			{% if entityModel.topNavHeader3 != '~' %}
				<span class="sub-level-heading">{{ entityModel.topNavHeader3|safe}}</span>
			{% else %}
				<span class="sub-level-heading">&nbsp;&nbsp;</span>
			{% endif %}
		{% endif %}
	{% endwith %}
{% endif %}
